<div class="user_wrapper">
  <% breadcrumb :users %>
  <%= render "shared/breadcrumbs" %>
  <div class="user_wrapper__header">
    <h3>ユーザ一覧</h3>
  </div>
  <div class="user_wrapper__content">
    <div class="user_wrapper__content__user_list">
      <ul>
        <% @users.each do |user| %>
          <li><%= link_to  user.name, user, class: 'btn btn-primary btn-users' %></li>
        <% end %>
      </ul>
    </div>
  </div>
</div>